<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/Updatedelivery/delivery/status" method="POST">
        <service class="Panama\MagentoApi\Api\UpdateDeliveryInterface" method="status"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
	<route url="/V1/Updateinvoices/invoices/update" method="POST">
        <service class="Panama\MagentoApi\Api\UpdateInvoicesInterface" method="updateInvoices"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>