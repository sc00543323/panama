/**
 * Shopping cart template
 *
 * @var $block \Magento\Checkout\Block\Cart
 */
$objectManager = \Magento\Framework\App\ObjectManager::getInstance();
$checkoutSession = $objectManager->create('Magento\Catalog\Model\Session');
echo "port->".$checkoutSession->getPort().'<br>';
echo "current_service ->".$checkoutSession->getCurrentService().'<br>';
echo "buy_smartphone->".$checkoutSession->getBuySmartphone();
